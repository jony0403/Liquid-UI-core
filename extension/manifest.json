{
  "manifest_version": 3,
  "name": "Liquid View God Mode",
  "version": "7.0",
  "permissions": ["activeTab", "scripting"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": { "128": "icon.png" }
  },
  "icons": {
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["logic.js", "ui.js"]
    }
  ]
}